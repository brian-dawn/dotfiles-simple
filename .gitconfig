[user]
    email = brian.t.dawn@gmail.com
    name = Brian Dawn
[pull]
	rebase = true
[init]
	defaultBranch = main
[rerere]
	enabled = true
[core]
	editor = vim
[alias]
	count-lines = "! git log --author=\"$1\" --pretty=tformat: --numstat | awk '{ add += $1; subs += $2; loc += $1 - $2 } END { printf \"added lines: %s, removed lines: %s, total lines: %s\\n\", add, subs, loc }' #"
[include]
    path = ~/.gitconfig-work

